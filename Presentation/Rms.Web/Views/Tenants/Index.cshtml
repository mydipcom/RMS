<!-- BEGIN PAGE HEADER-->
<h3 class="page-title">
    Tenants List
</h3>

<div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="index.html">Home</a>
            <i class="fa fa-angle-right"></i>
        </li>
        <li>
            <a href="#">Tenants</a>
        </li>
    </ul>
</div>
<!-- END PAGE HEADER-->
<!-- BEGIN PAGE CONTENT-->
<div class="row">
    <div class="col-md-12">
        <!-- Begin: life time stats -->
        <div class="portlet">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-shopping-cart"></i>Tenants Listing
                </div>
                <div class="actions">
                    <a href="#" class="btn default yellow-stripe" id="rowcreate">
                        <i class="fa fa-plus"></i>
                        <span class="hidden-480" id="rowcreate">
                            New Tenant
                        </span>
                    </a>
                </div>
            </div>
            <div class="portlet-body">
                <div class="table-container">
                    <table class="table table-striped table-bordered table-hover" id="datatable_ajax">
                        <thead>
                            <tr role="row" class="heading">
                                <th>
                                    Id
                                </th>
                                <th>
                                    TenantName
                                </th>
                                <th>
                                    MappedDomain
                                </th>
                                <th>
                                    Version
                                </th>
                                <th>
                                    Timezone
                                </th>
                                <th>
                                    IsPublic
                                </th>
                                <th>
                                    CreateDate
                                </th>
                                <th>
                                    Actions
                                </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- End: life time stats -->
    </div>
</div>
<!-- END PAGE CONTENT-->



<!-- Create or Update -->
<div id="createorupdate" class="modal fade" tabindex="-1" data-focus-on="input:first">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        <h4 class="modal-title">Tenants</h4>
    </div>
    <div class="modal-body">
        <div class="alert alert-danger" id="createorupdate-errormsg" style="display: none"></div>

        <form action="#" id="createorupdateform" class="form-horizontal">
            
            <div class="form-group">
                <label class="control-label col-md-4">TenantName</label>
                <div class="col-md-8">
                    <input class="form-control input-medium" name="TenantName" type="text" value="">
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-4">MappedDomain</label>
                <div class="col-md-8">
                    <input class="form-control input-medium" name="MappedDomain" type="text" value="">
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-4">Version</label>
                <div class="col-md-8">
                    <input class="form-control input-medium" name="Version" type="text" value="">
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-4">Timezone</label>
                <div class="col-md-8">
                    <input class="form-control input-medium" name="Timezone" type="text" value="">
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-4">IsPublic</label>
                <div class="col-md-8">
                    <input class="form-control input-medium " name="IsPublic" type="checkbox" >
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-md-4">CreateDate</label>
                <div class="col-md-8">
                    <input class="form-control input-medium" name="CreateDate" type="text" value="">
                </div>
            </div>
            <input name="Id" type="hidden" value="">
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" data-dismiss="modal" class="btn btn-default">Close</button>
        <button type="button" class="btn btn-primary" id="createorupdatesubmit">Ok</button> 
    </div>
</div>


<!-- static -->
<div id="item-delete" class="modal fade" tabindex="-1" data-backdrop="static" data-keyboard="false">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        <h4 class="modal-title">Are you sure?</h4>
    </div>
    <div class="modal-footer">
        <button type="button" data-dismiss="modal" class="btn btn-default">Cancel</button>
        <button type="button" data-dismiss="modal" class="btn blue" id="item-delete-ok">OK</button>
    </div>
</div>


