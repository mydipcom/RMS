                    <script>
                        var EXPAND_DURATION = 400;
                        var EXPLORE_TEXT_FADE_DURATION = 300;
                        var DELAY_BEFORE_FADE_BOXES = 100;
                        var FADE_BOXES_FADE_DELAY = 100;

                        $(document).ready(function () {
                            function expandExplore() {
                                scrollPageTo($('.mobile-explore-section'), EXPAND_DURATION);

                                $('.mobile-explore-section').addClass('expanded', EXPAND_DURATION);
                                $('.explore-text').addClass('expanded');
                                $('.mobile-explore-image').addClass('expanded', EXPAND_DURATION);

                                $('header').fadeOut(EXPAND_DURATION);
                                $('.close-mobile-explore').addClass('visible');
                                setTimeout(function () { $('.fade1').addClass('visible') }, EXPAND_DURATION);
                                setTimeout(function () { $('.fade3').addClass('visible') }, EXPAND_DURATION + 100);
                                setTimeout(function () { $('.fade4').addClass('visible') }, EXPAND_DURATION + 200);
                                setTimeout(function () { $('.fade2').addClass('visible') }, EXPAND_DURATION + 300);
                            }

                            function collapseExplore() {
                                scrollPageTo($('.mobile-explore-section'), EXPAND_DURATION, -100);

                                $('header').fadeIn(EXPAND_DURATION);
                                $('.mobile-explore-section').removeClass('expanded', EXPAND_DURATION);
                                $('.explore-text').removeClass('expanded');
                                $('.mobile-explore-image').removeClass('expanded', EXPAND_DURATION);
                                $('.close-mobile-explore').removeClass('visible');
                                $('.fade').removeClass('visible');
                            }

                            $('#mobile_explore_link').click(function () { expandExplore() });
                            $('.close-mobile-explore').click(function () { collapseExplore() });

                            $(window).resize(function () {
                                if ($(window).width() <= 1024 && $('.mobile-explore-section').hasClass('expanded')) {
                                    collapseExplore();
                                }
                            });
                        });

                        $(document).scroll(function () {
                            if ($('.mobile-explore-section').hasClass('expanded') && !isOnscreen($('.mobile-explore-section'))) {
                                fadeInHeader();
                            }
                        });
                    </script>
