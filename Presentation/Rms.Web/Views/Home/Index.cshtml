            $('#scheduling-signup .form-fields-list').height(formHeight());

            function formHeight() {
                if ($('#scheduling-signup .form-fields-list').hasClass('expanded'))
                    return $('#scheduling-signup .promo-code').outerHeight() * 3;
                else
                    return $('#scheduling-signup .promo-code').outerHeight() * 2;
            }

            $('#scheduling-signup #have_code').click(function () {
                $('#scheduling-signup .form-fields-list').toggleClass('expanded');
                var isExpanded = $('#scheduling-signup .form-fields-list').hasClass('expanded');
                if (isExpanded) {
                    $('#scheduling-signup .field-coupon input').prop('disabled', false);
                }
                $('#scheduling-signup .form-fields-list').animate({ 'height': formHeight() + 'px' }, DURATION, function () {
                    if (!isExpanded) {
                        $('#scheduling-signup .field-coupon input').prop('disabled', true);
                    }
                });
            });

            $(window).resize(function () {
                $('#scheduling-signup .form-fields-list').height(formHeight());
            });
        });
    </script>
