                $('#white-menu-icon').click(function () {
                    $('#menu').removeClass('expanded', MENU_EXPAND_DURATION);
                    $('.nav-items.gray').removeClass('hidden', MENU_EXPAND_DURATION);
                    $('#menu-icon').fadeIn(MENU_EXPAND_DURATION);
                });


                // Modernizr tests
                if (!Modernizr.svg) {
                    $("img[src$='.svg']").each(function (i, element) {
                        $(this).attr('src', $(this).attr('src').replace(".svg", ".png"));
                    });
                }
                if ("unknown".indexOf('ios') != -1 || !Modernizr.video) {
                    $('video').each(function (i, element) {
                        $(this).parents('.try-free').css({
                            'background-image': "url('" + $(this).find("source[src$='.mp4']").attr('src').replace('.mp4', '.jpg') + "')",
                            'background-size': 'cover'
                        });
                    });
                    $('video').remove();
                }
                if (!Modernizr.input.placeholder) {
                    $('input[placeholder]').each(function () {
                        $(this).addClass('placeholder');
                        $(this).val($(this).attr('placeholder'));
                        $(this).on({
                            focus: function () {
                                if ($(this).val() == $(this).attr('placeholder')) {
                                    $(this).val('');
                                    $(this).removeClass('placeholder');
                                }
                            },
                            blur: function () {
                                if ($(this).val() == '') {
                                    $(this).val($(this).attr('placeholder'));
                                    $(this).addClass('placeholder');
                                }
                            }
                        });
                    });
                }
                Modernizr.load([
