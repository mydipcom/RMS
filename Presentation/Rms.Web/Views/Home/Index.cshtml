        <script type="text/javascript">
            $(document).ready(function () {
                function ValidatePhone(phoneVal) {
                    var phoneRegExp = /^((\+)?[1-9]{1,2})?([-\s\.])?((\(\d{1,4}\))|\d{1,4})(([-\s\.])?[0-9]{1,12}){1,2}$/;
                    var numbers = phoneVal.length;
                    if (10 <= numbers && numbers <= 20 && phoneRegExp.test(phoneVal)) {
                        return true;
                    } else {
                        return false;
                    }
                }
                $('.text-form-submit').click(function (event) {
                    event.preventDefault();
                    var form = $(this).closest('form');
                    var phoneNum = $("input[name='textPhone']", form).val();
                    // alert($("input[name='textPhone']",form).val());
                    if (ValidatePhone(phoneNum)) {

                        form.find('.textPhone').hide();
                        form.find('.text-form-submit').hide();
                        form.find('.text-loading').show();
                        $.ajax({
                            url: '/text/textMe',
                            type: 'POST',
                            data: { phone: phoneNum },
                            success: function () {
                                form.find('.text-loading').hide();
                                form.find('.text-loaded').show();
                            },
                            error: function () {
                                alert("Sorry, there was an error processing your request. Please try again!");
                                form.find('.textPhone').show();
                                form.find('.text-form-submit').show();
                                form.find('.text-loading').hide();
                            }

                        });
                    } else {
                        $(this).find('.textPhone').addClass('error');
                    }


                });

            });


        </script>


